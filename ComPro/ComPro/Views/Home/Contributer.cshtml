@using ComPro.Helpers
@using Microsoft.AspNet.Identity
@model List<ComPro.Models.SiteContibuter>
@{
    ViewBag.Title = "Contributer";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>Contributers to this project</h3>
<div>
    @foreach (var c in Model)
    {
        <div class="row">
            <div class="col-md-2">
                <img src="" class="img-thumbnail"/>
            </div>
            <div class="col-md-10">
                <h4> @UserInformation.UserName(c.ContributingUser.Email)</h4>
                <p>
                    @c.Contribution
                </p>
            </div>
        </div>
    }
</div>

